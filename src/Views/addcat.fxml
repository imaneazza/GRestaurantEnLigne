<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.addcatController">
   <children>
      <MenuBar fx:id="menu" prefHeight="30.0" prefWidth="620.0">
         <menus>
            <Menu mnemonicParsing="false" text="Gérer les Offres">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#GoToOffre" text="Créer un offre" />
                  <MenuItem mnemonicParsing="false" onAction="#gotoliste" text="Liste des Offres" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Gérer les Ingrédients">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#catGerer" text="Gérer les Catégories " />
                  <MenuItem mnemonicParsing="false" text="Gérer les Ingrédients" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Activer les Offres" />
         </menus>
      </MenuBar>
      <Pane fx:id="pan1" layoutX="-1.0" layoutY="30.0" prefHeight="93.0" prefWidth="620.0" style="-fx-background-color: red;">
         <children>
            <ImageView fitHeight="73.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pictures/logo.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane fx:id="pan11" layoutY="122.0" prefHeight="278.0" prefWidth="620.0" style="-fx-background-color: white;">
         <children>
            <TextField fx:id="txtname" layoutX="278.0" layoutY="55.0" prefHeight="30.0" prefWidth="231.0" />
            <Label layoutX="50.0" layoutY="52.0" prefHeight="36.0" prefWidth="116.0" text="Nom de catégorie" />
            <Label layoutX="44.0" layoutY="121.0" prefHeight="36.0" prefWidth="116.0" text="Image de catégorie" />
            <Button fx:id="conf" layoutX="306.0" layoutY="199.0" mnemonicParsing="false" onAction="#AddCat" prefHeight="43.0" prefWidth="166.0" text="Confirmer" />
            <Button fx:id="openButton" layoutX="500.0" layoutY="132.0" mnemonicParsing="false" text="Open" />
            <Label fx:id="labelx" layoutX="278.0" layoutY="121.0" prefHeight="36.0" prefWidth="222.0" text="Label" />
            <Button fx:id="annuler" layoutX="50.0" layoutY="199.0" mnemonicParsing="false" onAction="#annuler" prefHeight="43.0" prefWidth="166.0" text="Annuler" />
         </children>
      </Pane>
   </children>
</Pane>
